{
  "name": "@webstudio-is/trpc-interface",
  "version": "0.91.0",
  "description": "Webstudio TRPC Interface",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "type": "module",
  "scripts": {
    "typecheck": "tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "checks": "pnpm typecheck",
    "dev": "build-package --watch",
    "build": "build-package",
    "build-tsc": "pnpm build-tsc:esm",
    "build-tsc:esm": "tsc --project tsconfig.tsc.json",
    "build-tsc:cjs": "tsc --module commonjs --target es5 --outDir lib/cjs",
    "dts": "tsc --project tsconfig.dts.json"
  },
  "dependencies": {
    "@trpc/client": "^10.38.1",
    "@trpc/server": "^10.38.1",
    "@webstudio-is/prisma-client": "workspace:^",
    "ts-custom-error": "^3.3.1",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@webstudio-is/jest-config": "workspace:^",
    "@webstudio-is/scripts": "workspace:^",
    "@webstudio-is/tsconfig": "workspace:^",
    "typescript": "5.1.6"
  },
  "exports": {
    "./index.server": {
      "source": "./src/index.server.ts",
      "import": "./lib/index.server.js"
    }
  },
  "files": [
    "lib/*",
    "src/*",
    "!*.test.*"
  ],
  "license": "AGPL-3.0-or-later",
  "private": false,
  "sideEffects": false
}
