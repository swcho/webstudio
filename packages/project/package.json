{
  "name": "@webstudio-is/project",
  "version": "0.91.0",
  "description": "Webstudio Project",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "type": "module",
  "scripts": {
    "typecheck": "tsc",
    "checks": "pnpm typecheck",
    "dev": "build-package --watch",
    "build": "build-package",
    "build-tsc": "pnpm build-tsc:esm",
    "build-tsc:esm": "tsc --project tsconfig.tsc.json",
    "build-tsc:cjs": "tsc --module commonjs --target es5 --outDir lib/cjs",
    "dts": "tsc --project tsconfig.dts.json"
  },
  "dependencies": {
    "@trpc/server": "^10.38.1",
    "@webstudio-is/asset-uploader": "workspace:^",
    "@webstudio-is/prisma-client": "workspace:^",
    "@webstudio-is/project-build": "workspace:^",
    "@webstudio-is/trpc-interface": "workspace:^",
    "nanoid": "^4.0.2",
    "slugify": "^1.6.6",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "@webstudio-is/scripts": "workspace:^",
    "@webstudio-is/tsconfig": "workspace:^",
    "typescript": "5.1.6"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./lib/types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./index.server": {
      "source": "./src/index.server.ts",
      "types": "./lib/types/index.server.d.ts",
      "import": "./lib/index.server.js"
    }
  },
  "files": [
    "lib/*",
    "src/*"
  ],
  "license": "AGPL-3.0-or-later",
  "private": false,
  "sideEffects": false
}
